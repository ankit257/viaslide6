<script>
var old_li='#album-thumb-0';
function next(){
var n = old_li.split("-");
var d = parseInt(n[2])+1;
var m = n[0]+'-'+n[1]+'-'+d;
document.getElementById(old_li).style.opacity = "1.0";
change_content(m);
if (old_li!= null){
        
      }
     document.getElementById(m).style.opacity = "0.4";
          old_li = m;
};

function change_content(id){
    //alert(id);
    var x = '#'+id;
    //alert(x);
    var src = $("img",x).attr("src");
    var txt = $("img",x).attr("alt");
    alert(src);
    var image_new = src.replace("_0","_2");
    var image = '<img src="'+ image_new +'"></img>';
          $("#albumImage").html(image);
          $("#slidetext").html(txt);
          old_li = id;
        
};

function prev(){
var n = old_li.split("-");
var d = parseInt(n[2])-1;
var m = n[0]+'-'+n[1]+'-'+d;
document.getElementById(old_li).style.opacity = "1.0";
change_content(m);
document.getElementById(m).style.opacity = "0.4";
old_li = m;
};
</script>



